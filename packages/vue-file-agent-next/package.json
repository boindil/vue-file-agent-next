{
  "name": "@boindil/vue-file-agent-next",
  "displayName": "VueFileAgentNext",
  "description": "VueFileAgent for Vue 3",
  "version": "1.0.0",
  "license": "MIT",
  "main": "./dist/vue-file-agent-next.umd.js",
  "module": "./dist/vue-file-agent-next.es.js",
  "exports": {
    ".": {
      "import": "./dist/vue-file-agent-next.es.js",
      "require": "./dist/vue-file-agent-next.umd.js"
    },
    "./dist/vue-file-agent-next.css": "./dist/vue-file-agent-next.css",
    "./nuxt": "./nuxt.js",
    "./src/scss/vue-file-agent-next.scss": "./src/scss/vue-file-agent-next.scss"
  },
  "files": [
    "dist",
    "src",
    "nuxt.js"
  ],
  "types": "./dist/VueFileAgent.d.ts",
  "private": false,
  "scripts": {
    "dev": "vite --port 5174",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "lint": "eslint --ext .js,.vue --ignore-path ../../.gitignore --fix src",
    "format": "prettier . --write",
    "test": "pnpm run test:lint && pnpm run test:unit",
    "test:lint": "pnpm run lint",
    "test:unit": "vitest",
    "test:coverage": "vitest --coverage"
  },
  "peerDependencies": {
    "@popperjs/core": "^2.11.6",
    "vue": "^3.2.43"
  },
  "dependencies": {
    "@nuxt/kit": "3.0.0-rc.12"
  },
  "devDependencies": {
    "@popperjs/core": "^2.11.6",
    "@vitejs/plugin-vue": "^3.0.0",
    "@vitest/coverage-c8": "^0.23.2",
    "@vue/runtime-core": "^3.2.43",
    "@vue/shared": "^3.2.43",
    "@vue/test-utils": "^2.0.2",
    "c8": "^7.12.0",
    "happy-dom": "^7.5.12",
    "jsdom": "^20.0.0",
    "rollup": "^2.78.1",
    "rollup-plugin-visualizer": "^5.7.1",
    "sass": "^1.54.4",
    "tsconfig": "workspace:*",
    "tus-js-client": "^3.0.1",
    "typescript": "^4.7.4",
    "vite": "^3.0.0",
    "vite-plugin-dts": "^1.4.1",
    "vitest": "^0.23.2",
    "vue": "^3.2.43",
    "vue-router": "^4.1.3",
    "vue-slicksort": "^2.0.0-alpha.5",
    "vue-tsc": "^1.0.9"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/boindil/vue-file-agent-next.git"
  },
  "bugs": {
    "url": "https://github.com/boindil/vue-file-agent-next/issues"
  },
  "homepage": "https://github.com/boindil/vue-file-agent-next",
  "keywords": [
    "vue3",
    "vue",
    "components",
    "typescript",
    "component-library",
    "VueFileAgent"
  ],
  "lint-staged": {
    "*.{js,vue}": "eslint --cache --fix",
    "*": "prettier --write --ignore-unknown"
  }
}
